<view class="index-container">
  <view class="header">
    <van-search bindtap="clickSearch" disabled bind:focus="clickSearch" value="{{ value }}" shape="round" background="#fa2c19" placeholder="请输入搜索关键词" />

    <swiper swiperData="{{ swiperData }}" indicatorDots="{{ indicatorDots }}" autoplay="{{ autoplay }}" interval="{{ interval }}" duration="{{ duration }}"></swiper>

  </view>
  <view class="nav">
    <van-grid column-num="4">
      <van-grid-item wx:key="index" icon-color="{{ item.color }}" icon="{{ item.icon }}" text="{{ item.text }}" wx:for="{{ navData }}" />
    </van-grid>
  </view>




</view>